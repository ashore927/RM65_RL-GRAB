<?xml version="1.0" ?>
<!-- WSG50 夹爪 URDF - 从 SDF 转换 -->
<!-- 关键控制关节: gripper_left_hinge_joint, gripper_right_hinge_joint -->
<!-- 左手指: 负值张开，正值闭合 -->
<!-- 右手指: 正值张开，负值闭合 -->
<robot name="wsg50_with_gripper">

  <!-- World link (虚拟基座) -->
  <link name="world"/>

  <!-- X轴移动链接 -->
  <link name="x_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.2"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>

  <joint name="x_axis" type="prismatic">
    <parent link="world"/>
    <child link="x_link"/>
    <origin xyz="0 0 0.7" rpy="3.14159 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-10" upper="10" effort="1" velocity="1"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- Y轴移动链接 -->
  <link name="y_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.2"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>

  <joint name="y_axis" type="prismatic">
    <parent link="x_link"/>
    <child link="y_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-10" upper="10" effort="1" velocity="1"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- Z轴移动链接 -->
  <link name="z_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.2"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>

  <joint name="z_axis" type="prismatic">
    <parent link="y_link"/>
    <child link="z_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-10" upper="10" effort="1" velocity="1"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 基座旋转链接 -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.2"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/WSG50_110.stl" scale="1 1 1"/>
      </geometry>
      <material name="grey">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_joint" type="revolute">
    <parent link="z_link"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="1" velocity="1"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 电机链接 -->
  <link name="motor">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.01" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.3 0.3 0.3 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_joint_motor" type="prismatic">
    <parent link="base_link"/>
    <child link="motor"/>
    <origin xyz="0 0 0.03" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.055" upper="0.001" effort="10" velocity="10"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 左铰链 -->
  <link name="left_hinge">
    <inertial>
      <origin xyz="0 0 0.035" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="-0.03 0 0.01" rpy="0 -1.2 0"/>
      <geometry>
        <box size="0.02 0.02 0.07"/>
      </geometry>
      <material name="dark_grey"/>
    </visual>
  </link>

  <joint name="motor_left_hinge_joint" type="revolute">
    <parent link="motor"/>
    <child link="left_hinge"/>
    <origin xyz="0 0 0.01" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-20" upper="20" effort="10" velocity="10"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 右铰链 -->
  <link name="right_hinge">
    <inertial>
      <origin xyz="0 0 0.035" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0.03 0 0.01" rpy="0 1.2 0"/>
      <geometry>
        <box size="0.02 0.02 0.07"/>
      </geometry>
      <material name="dark_grey"/>
    </visual>
  </link>

  <joint name="motor_right_hinge_joint" type="revolute">
    <parent link="motor"/>
    <child link="right_hinge"/>
    <origin xyz="0 0 0.01" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-20" upper="20" effort="10" velocity="10"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 左夹爪滑块 -->
  <link name="gripper_left">
    <inertial>
      <origin xyz="0 0 0.0115" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <visual>
      <origin xyz="0 0 -0.037" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/WSG-FMF.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>

  <joint name="gripper_left_hinge_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="gripper_left"/>
    <origin xyz="-0.055 0 0.06" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.01" upper="0.05" effort="1" velocity="1"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 右夹爪滑块 -->
  <link name="gripper_right">
    <inertial>
      <origin xyz="0 0 0.0115" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.06" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <visual>
      <origin xyz="0 0 -0.037" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/WSG-FMF.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>

  <joint name="gripper_right_hinge_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="gripper_right"/>
    <origin xyz="0.055 0 0.06" rpy="0 0 3.14159"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.01" upper="0.05" effort="1" velocity="1"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <!-- 左手指 -->
  <!-- SDF: finger_left 全局位置 (-0.062, 0, 0.145), 旋转 (0, 0, 4.71239) -->
  <!-- SDF: gripper_left 全局位置 (-0.055, 0, 0.06), 旋转 (0, 0, 0) -->
  <!-- 相对位置: (-0.062 - (-0.055), 0, 0.145 - 0.06) = (-0.007, 0, 0.085) -->
  <link name="finger_left">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/l_gripper_tip_scaled.stl" scale="1 1 1"/>
      </geometry>
      <material name="blue">
        <color rgba="0.2 0.2 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.042" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.15"/>
      </geometry>
    </collision>
  </link>

  <joint name="gripper_finger_left" type="fixed">
    <parent link="gripper_left"/>
    <child link="finger_left"/>
    <origin xyz="-0.007 0 0.085" rpy="0 0 4.71239"/>
  </joint>

  <!-- 右手指 -->
  <!-- SDF: finger_right 全局位置 (0.062, 0, 0.145), 旋转 (0, 0, 1.5708) -->
  <!-- SDF: gripper_right 全局位置 (0.055, 0, 0.06), 旋转 (0, 0, 3.14159) -->
  <!-- gripper_right 绕Z轴旋转180度，局部坐标需要反转 X 和 Y -->
  <!-- 全局差值: (0.007, 0, 0.085), 局部: (-0.007, 0, 0.085) -->
  <link name="finger_right">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="meshes/l_gripper_tip_scaled.stl" scale="1 1 1"/>
      </geometry>
      <material name="blue">
        <color rgba="0.2 0.2 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.042" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.15"/>
      </geometry>
    </collision>
  </link>

  <joint name="gripper_finger_right" type="fixed">
    <parent link="gripper_right"/>
    <child link="finger_right"/>
    <!-- 因为 gripper_right 旋转了180度，局部坐标X反转，旋转也需调整 -->
    <origin xyz="-0.007 0 0.085" rpy="0 0 -1.5708"/>
  </joint>

</robot>
